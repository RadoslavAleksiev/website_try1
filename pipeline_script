pipeline {
  agent any
  
  stages {
    stage('Clone Repository') {
      steps {
        git clone 'https://github.com/RadoslavAleksiev/website_try1.git' branch: "main"
      }
    }
    
    stage('Build') {
      steps {
        sh 'npm install'  // Or any other build commands specific to your project
      }
    }
    
    stage('Deploy') {
      steps {
        sh 'npm run build'  // Or any other deployment commands specific to your project
      }
    }
  }
}
